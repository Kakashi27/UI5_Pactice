<mvc:View controllerName="student27.com.sap.training.ux402.fullscreen.ux402fullscreen.controller.Carrier"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="carrierPage" title="{i18n>overviewPageTitle}">
        <content >
            <Table id="carrierTable" items="{/UX_C_Carrier_TP}">
                <columns>
                    <Column id="column1">
                        <Text id="text1" text="{i18n>columnId}"></Text>
                    </Column>
                    <Column id="column2">
                        <Text id="text2" text="{i18n>columnName}"></Text>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="columnListItem1">
                        <cells>
                            <ObjectIdentifier id="objectIdentifier1" title="{Carrid}"></ObjectIdentifier>
                            <Text id="text3" text="{Carrname}"></Text>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
